<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite thread-count="3" parallel="tests" name="ParallelCrossBrowserSuite">
	<test name="Chromium Test">
		<parameter name="browserName" value="chromium" />
		<classes>
			<class name="learn.test3.CrossBrowserTest" />
		</classes>
	</test>
	<test name="Firefox Test">
		<parameter name="browserName" value="firefox" />
		<classes>
			<class name="learn.test3.CrossBrowserTest" />
		</classes>
	</test>
	<test name="WebKit Test">
		<parameter name="browserName" value="webkit" />
		<classes>
			<class name="learn.test3.CrossBrowserTest" />
		</classes>
	</test>
</suite>